	TrajectoryLinkingWindow -> TrajectoryPlottingWidget [label=has]
	TrajectoryLinkingWindow -> TrajectoryPlayerWidget [label=has]
	TrajectoryLinkingWindow -> ErrantTrajectoryGalleryWidget [label=has]
