	GraphingPanelWidget [label="{GraphingPanelWidget|}"]
	FramePlayerWidget [label="{FramePlayerWidget|+ extract_frames()\l+ select_frame()\l+ display_frame()\l}"]
	ErrantParticleGalleryWidget [label="{ErrantParticleGalleryWidget|+ next_particle()\l+ prev_particle()\l}"]
