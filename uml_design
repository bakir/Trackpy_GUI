	TrajectoryPlottingWidget [label="{TrajectoryPlottingWidget|}"]
	TrajectoryPlayerWidget [label="{TrajectoryPlayerWidget|+ display_trajectory_plot()}"]
	ErrantTrajectoryGalleryWidget [label="{ErrantTrajectoryGalleryWidget|+ load_rb_gallery_files()\l+ next_trajectory()\l+ prev_trajectory()\l}"]
